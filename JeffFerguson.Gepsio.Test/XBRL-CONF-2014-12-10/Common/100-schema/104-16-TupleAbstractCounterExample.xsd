<xsd:schema targetNamespace="http://mycompany.com/xbrl/taxonomy" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://mycompany.com/xbrl/taxonomy" xmlns:xbrli="http://www.xbrl.org/2003/instance" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:import namespace="http://www.xbrl.org/2003/instance" schemaLocation="http://www.xbrl.org/2003/xbrl-instance-2003-12-31.xsd"/>
	<xsd:element name="managementName" type="xbrli:tokenItemType" substitutionGroup="xbrli:item"  xbrli:periodType="instant"/>
	<xsd:element name="managementTitle" type="xbrli:tokenItemType" substitutionGroup="xbrli:item"  xbrli:periodType="instant"/>
	<xsd:element name="managementAge" type="xbrli:nonNegativeIntegerItemType" substitutionGroup="xbrli:item"  xbrli:periodType="instant"/>
	<xsd:element name="managementInformation" abstract="true" substitutionGroup="xbrli:tuple">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">
					<xsd:sequence>
						<xsd:element ref="managementName"/>
						<xsd:element ref="managementTitle"/>
						<xsd:element ref="managementAge"/>
					</xsd:sequence>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
