<?xml version="1.0" encoding="utf-8"?>

<xbrl xmlns="http://www.xbrl.org/2003/instance" xmlns:link="http://www.xbrl.org/2003/linkbase" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:iso4217="http://www.xbrl.org/2003/iso4217" xmlns:calcinferprecisiontestcase="http://www.example.com/calcinferprecisiontestcase">
  <link:schemaRef xlink:href="320-BindCalculationInferPrecision.xsd" xlink:type="simple" />
  <context id="context">
    <entity>
      <identifier scheme="http://www.entity.com">entity</identifier>
    </entity>
    <period>
      <instant>2006-07-28</instant>
    </period>
  </context>
  <unit id="unit">
    <measure>iso4217:USD</measure>
  </unit>
  <calcinferprecisiontestcase:A contextRef="context" unitRef="unit" decimals="0">4000</calcinferprecisiontestcase:A>

  <calcinferprecisiontestcase:B contextRef="context" unitRef="unit" decimals="0">4000</calcinferprecisiontestcase:B>
	
	<!-- 0.01 with decimals zero infer as 0 -->
  <calcinferprecisiontestcase:C contextRef="context" unitRef="unit" decimals="0">0.01</calcinferprecisiontestcase:C>
</xbrl>